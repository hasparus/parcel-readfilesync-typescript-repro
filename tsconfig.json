{
  "compilerOptions": {
    "jsx": "react",
    "newLine": "LF",
    "sourceMap": true,
    "declaration": true,
    "lib": ["es2017", "dom"],
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  }
}
